<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:Acl/etc/acl.xsd">
    <acl>
        <resources>
            <resource id="Magento_Backend::admin">
                <resource id="Swissup_Core::swissup" title="Swissup">
                    <resource id="Swissup_Core::services" title="Services" sortOrder="0">
                        <resource id="Swissup_Core::installer_index" title="Module Manager" sortOrder="10" />
                        <resource id="Swissup_Core::installer_form" title="Installer Form" sortOrder="20" />
